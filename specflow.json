{
  "specFlow": {
    "language": {
      "feature": "en"
    },
    "bindingCulture": {
      "name": "en"
    },
    "unitTestProvider": {
      "name": "mstest"
    },
    "plugins": [
      {
        "name": "SpecFlow.MsTest",
        "type": "Runtime"
      },
      {
        "name": "SpecFlow.Plus.LivingDoc"
      }
    ],
    "stepAssemblies": [
      {
        "assembly": "SeleniumWebDriverNET"
      }
    ],
    "runtime": {
      "execution": {
        "parallelExecution": true,
        "testThreadCount": 4
      }
    }
  }
}
